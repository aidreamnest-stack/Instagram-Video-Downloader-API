{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api",
            "dest": "/index.js"
        },
        {
            "src": "/igdl",
            "dest": "/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        },
        {
            "src": "/",
            "dest": "/public/index.html"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}